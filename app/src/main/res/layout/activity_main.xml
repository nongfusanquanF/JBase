<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android">

    <data>

        <variable
            name="onclick"
            type="com.appbyme.jbase.MainActivity.UiClick" />
    </data>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <Button
            android:id="@+id/list_activity"
            android:onClick="@{onclick.uiClick}"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="list_Activity" />

        <Button
            android:id="@+id/list_fragment"
            android:onClick="@{onclick.uiClick}"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="list_fragment" />

        <Button
            android:id="@+id/details_activity"
            android:onClick="@{onclick.uiClick}"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="details_Activity" />

        <Button
            android:id="@+id/details_fragment"
            android:onClick="@{onclick.uiClick}"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="details_fragment" />


        <TextView
            android:id="@+id/title"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textSize="16dp"
            android:text="ddddddd"
            android:textColor="#f00"
            />
    </LinearLayout>




</layout>
